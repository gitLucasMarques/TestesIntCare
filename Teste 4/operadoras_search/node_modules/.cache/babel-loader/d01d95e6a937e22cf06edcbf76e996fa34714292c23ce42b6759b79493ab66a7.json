{"ast":null,"code":"import ResultsTable from './components/ResultsTable.vue';\nexport default {\n  components: {\n    ResultsTable\n  },\n  data() {\n    return {\n      searchTerm: '',\n      results: [],\n      loading: false,\n      error: null,\n      searchPerformed: false,\n      lastSearchTerm: ''\n    };\n  },\n  mounted() {\n    this.$refs.searchInput.focus();\n  },\n  methods: {\n    async handleSearch() {\n      const query = this.searchTerm.trim();\n      this.lastSearchTerm = query;\n      if (query.length < 2) {\n        this.error = \"Digite pelo menos 2 caracteres\";\n        this.results = [];\n        return;\n      }\n      this.loading = true;\n      this.error = null;\n      this.searchPerformed = true;\n      try {\n        const response = await fetch(`http://localhost:5000/api/search?q=${encodeURIComponent(query)}`);\n        if (!response.ok) throw new Error(\"Erro na busca\");\n        this.results = await response.json();\n      } catch (err) {\n        this.error = \"Erro ao conectar com o servidor\";\n        console.error(err);\n        this.results = [];\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["ResultsTable","components","data","searchTerm","results","loading","error","searchPerformed","lastSearchTerm","mounted","$refs","searchInput","focus","methods","handleSearch","query","trim","length","response","fetch","encodeURIComponent","ok","Error","json","err","console"],"sources":["/Users/lucasmarques/Desktop/teste/operadoras_search/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header class=\"app-header\">\n      <h1>Busca de Operadoras de Sa√∫de</h1>\n      <p class=\"subtitle\">\n        Pesquise por <strong>Nome da Operadora</strong> (ex: \"Sorriso\")\n      </p>\n    </header>\n\n    <main class=\"app-content\">\n      <div class=\"search-container\">\n        <input\n          v-model=\"searchTerm\"\n          @keyup.enter=\"handleSearch\"\n          placeholder=\"Digite o nome da operadora...\"\n          ref=\"searchInput\"\n        />\n        <button @click=\"handleSearch\" :disabled=\"loading\">\n          {{ loading ? 'Buscando...' : 'Buscar' }}\n        </button>\n      </div>\n\n      <div v-if=\"error\" class=\"error-message\">\n        {{ error }}\n      </div>\n\n      <div v-if=\"searchPerformed && results.length === 0\" class=\"no-results\">\n        Nenhum resultado encontrado para \"{{ lastSearchTerm }}\"\n      </div>\n\n      <ResultsTable \n        :results=\"results\" \n        :loading=\"loading\"\n        v-if=\"results.length > 0\"\n      />\n    </main>\n  </div>\n</template>\n\n<script>\nimport ResultsTable from './components/ResultsTable.vue'\n\nexport default {\n  components: { ResultsTable },\n  data() {\n    return {\n      searchTerm: '',\n      results: [],\n      loading: false,\n      error: null,\n      searchPerformed: false,\n      lastSearchTerm: ''\n    }\n  },\n  mounted() {\n    this.$refs.searchInput.focus()\n  },\n  methods: {\n    async handleSearch() {\n      const query = this.searchTerm.trim()\n      this.lastSearchTerm = query\n      \n      if (query.length < 2) {\n        this.error = \"Digite pelo menos 2 caracteres\"\n        this.results = []\n        return\n      }\n      \n      this.loading = true\n      this.error = null\n      this.searchPerformed = true\n      \n      try {\n        const response = await fetch(\n          `http://localhost:5000/api/search?q=${encodeURIComponent(query)}`\n        )\n        \n        if (!response.ok) throw new Error(\"Erro na busca\")\n        \n        this.results = await response.json()\n        \n      } catch (err) {\n        this.error = \"Erro ao conectar com o servidor\"\n        console.error(err)\n        this.results = []\n      } finally {\n        this.loading = false\n      }\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.app-header {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.app-header h1 {\n  color: #2c7be5;\n  margin-bottom: 10px;\n}\n\n.subtitle {\n  color: #6c757d;\n  font-size: 1.1rem;\n}\n\n.search-container {\n  display: flex;\n  gap: 10px;\n  max-width: 600px;\n  margin: 0 auto 30px;\n}\n\n.search-container input {\n  flex: 1;\n  padding: 12px 15px;\n  border: 2px solid #dfe2e6;\n  border-radius: 6px;\n  font-size: 1rem;\n  transition: border-color 0.3s;\n}\n\n.search-container input:focus {\n  border-color: #2c7be5;\n  outline: none;\n}\n\n.search-container button {\n  padding: 0 25px;\n  background-color: #2c7be5;\n  color: white;\n  border: none;\n  border-radius: 6px;\n  font-weight: 600;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.search-container button:hover {\n  background-color: #1a68d1;\n}\n\n.search-container button:disabled {\n  background-color: #95aac9;\n  cursor: not-allowed;\n}\n\n.error-message {\n  color: #e63757;\n  text-align: center;\n  margin: 10px 0;\n  font-weight: 500;\n}\n\n.no-results {\n  text-align: center;\n  color: #6c757d;\n  margin: 30px 0;\n  font-size: 1.1rem;\n}\n</style>"],"mappings":"AAwCA,OAAOA,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,UAAU,EAAE;IAAED;EAAa,CAAC;EAC5BE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE,IAAI;MACXC,eAAe,EAAE,KAAK;MACtBC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,KAAK,CAACC,WAAW,CAACC,KAAK,CAAC;EAC/B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAI,IAAI,CAACZ,UAAU,CAACa,IAAI,CAAC;MACnC,IAAI,CAACR,cAAa,GAAIO,KAAI;MAE1B,IAAIA,KAAK,CAACE,MAAK,GAAI,CAAC,EAAE;QACpB,IAAI,CAACX,KAAI,GAAI,gCAA+B;QAC5C,IAAI,CAACF,OAAM,GAAI,EAAC;QAChB;MACF;MAEA,IAAI,CAACC,OAAM,GAAI,IAAG;MAClB,IAAI,CAACC,KAAI,GAAI,IAAG;MAChB,IAAI,CAACC,eAAc,GAAI,IAAG;MAE1B,IAAI;QACF,MAAMW,QAAO,GAAI,MAAMC,KAAK,CAC1B,sCAAsCC,kBAAkB,CAACL,KAAK,CAAC,EACjE;QAEA,IAAI,CAACG,QAAQ,CAACG,EAAE,EAAE,